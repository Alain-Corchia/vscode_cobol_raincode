{
  "fileTypes": [],
  "name": "COBOL_LISTFILE",
  "patterns": [
    {
      "match" : "(\\f)",
      "name" : "strong comment.line.form_feed.cobol_listfile"
    },
    {
      "match": "(^[0-9 \\*][0-9 \\*][0-9 \\*][0-9 \\*][0-9 \\*][0-9 \\*])([pP])(.*)",
      "captures": {
        "1": {
          "name": "constant.numeric.cobol_listfile"
        },
        "2": {
          "name": "comment.block.preprocessor.cobol_listfile"
        },
        "3" : {
          "name" : "emphasis bold"
        }
      }
    },
    {

    },
    {
      "include": "source.cobol"
    }
  ],
  "scopeName": "source.cobol_listfile"
}