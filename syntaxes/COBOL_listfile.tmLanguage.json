  "fileTypes": [],
  "name": "COBOL_LISTFILE",
  "patterns": [
    {
      "match": "(\\f)",
      "name": "strong comment.line.form_feed.cobol_listfile"
    },
    {
      "match": "(^\\*.*$)",
      "name": "comment.line.modern"
    },
    {
      "begin": "(^[0-9 ][0-9 ][0-9 ][0-9 ][0-9 ][0-9 ])",
      "name": "constant.numeric.cobol_listfile",
      "patterns": [
        {
          "include": "source.cobol"
        }
      ],
      "end": "$"
    }
  ],
  "scopeName": "source.cobol_listfile"
}