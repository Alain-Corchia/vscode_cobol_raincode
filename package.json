{
	"name": "cobol",
	"description": "COBOL syntax support",
	"version": "0.4.3",
	"publisher": "bitlang",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "images/cobol.png",
	"categories": [
		"Languages"
	],
	"contributes": {
		"configurationDefaults": {
			"[COBOL]": {
				"editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
			}
		},
		"languages": [
			{
				"id": "COBOL",
				"aliases": [
					"COBOL",
					"cobol"
				],
				"extensions": [
					".cbl",
					".cpy",
					".cob",
					".dds",
					".ss",
					".wks",
					".pco"
				],
				"configuration": "./cobol.configuration.json"
			},
			{
				"id": "JCL",
				"aliases": [
					"JCL",
					"jcl"
				],
				"extensions": [
					".jcl"
				],
				"configuration": "./jcl.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "COBOL",
				"scopeName": "source.cobol",
				"path": "./syntaxes/cobol.tmLanguage"
			},
			{
				"language": "JCL",
				"scopeName": "source.jcl",
				"path": "./syntaxes/jcl.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "COBOL",
				"path": "./snippets/cobol.json"
			}
		]
	}
}